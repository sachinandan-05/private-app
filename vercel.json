{
    "version": 2,
    "builds": [
        {
            "src": "private_info/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "private_info/wsgi.py"
        }
    ],
    "static": {
        "src": "static/",
        "dest": "static/"
    }
}
